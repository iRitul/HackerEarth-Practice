#include<stdio.h>
 main()
 {
   int a[100000],b[100000],i,n,total[100000];
   scanf("%d",&n);
   for(i=0;i<n;i++)
   {
   scanf("%d",&a[i]);
   }
   for(i=0;i<n;i++)
   {
   scanf("%d",&b[i]);
   }
   for(i=0;i<n;i++)
   {
   total[i]=a[i]+b[i];
   }
   for(i=0;i<n;i++)
   {
   printf(" %d",total[i]);
   
 }
   }
